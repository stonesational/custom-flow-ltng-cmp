<!--    c:CsSaveSessionEvt
        Component scoped event CsHome is handling and always above in the component hierachy. Less noisey
-->
<aura:event type="COMPONENT" description="Send session to parent(CsHome) so it can be saved to the server">
        
        <aura:attribute description="The current session from the current Panel"
                name="session" 
                type="Object" />
</aura:event>
